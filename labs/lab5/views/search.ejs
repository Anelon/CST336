<% include partials/header.ejs %>
<form action="/search">
      <input type="text" id="keyword" placeholder="Type Keyword" name="keyword" value="<%= keyword %>">
      <button>Search</button>
</form>
<a id="favLink" href="/favorites">Show Favorites</a>

<% imgUrls.forEach(function(imgUrl, i) { %>

      <%- ((i%3 == 0))?'<br>':'' %>
      <card>
            <img class="img" src='<%= imgUrl%>' height='200'>
            <img class='fav' src='img/fav_off.png' width='20'>
      </card>
      
<% });%>

<% include partials/footer.ejs %>
